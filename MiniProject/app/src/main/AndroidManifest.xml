<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cookandroid.miniproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/diary"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/diary"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".FingerPrintActivity"></activity>
        <activity android:name=".IDListActivity" />
        <activity android:name=".UpdateActivity" />
        <activity android:name=".Detail" />
        <activity android:name=".WriteActivity" />
        <activity android:name=".index" />
        <activity android:name=".intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>